<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/history_navigation"
    app:startDestination="@id/historyParentFragment">

    <fragment
        android:id="@+id/dialogFilterFragment"
        android:name="com.capstone.turuappmobile.ui.fragment.historyList.DialogFilterFragment"
        android:label="fragment_dialog_filter"
        tools:layout="@layout/fragment_dialog_filter" />
    <fragment
        android:id="@+id/historyParentFragment"
        android:name="com.capstone.turuappmobile.ui.fragment.history.HistoryParentFragment"
        android:label="fragment_history_parent"
        tools:layout="@layout/fragment_history_parent" >
        <action
            android:id="@+id/action_historyParentFragment_to_dialogFilterFragment"
            app:destination="@id/dialogFilterFragment" />
    </fragment>
</navigation>